#include "holberton.h"
/**
 * rev_string - check the code for Holberton School students.
 * @s: char pointer to be determined.
 * Return: void.
 */
int sqr(int a, int v)
{
	if (a * a == v)
	{
		return (a);
	}
	else if (a * a > v)
	{
		return (-1);
	}
	return (sqr(a + 1, v));
}

int _sqrt_recursion(int n)
{
	return (sqr(1, n));
}
